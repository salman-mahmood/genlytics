{
    "name": "zfhassaan/genlytics",
    "description": "Genlytics is a powerful Google Analytics Composer package that allows for easy data collection, analysis, and visualization. Optimized for SEO, it helps improve website performance and drive more organic traffic.",
    "type": "library",
    "require": {
        "google/apiclient": "^2.12",
        "php": "^8.1"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "zfhassaan\\genlytics\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Hassaan Ali",
            "email": "zfhassaan@gmail.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "zfhassaan\\genlytics\\Provider\\AnalyticsServiceProvider"
            ],
            "aliases": {
                "genlytics": "zfhassaan\\genlytics\\Facades\\AnalyticsFacade"
            }
        }
    }
}
